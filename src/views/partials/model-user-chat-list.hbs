{{#each allUser}}
<a href="/chat/{{this.emisor.receptor.user._id}}">
    <div class="target-user-list d-flex align-items-center justify-content-between p-2 mb-3">
        <div class="target-chat-image-name">
            <img class="rounded-circle" src="/img/user/{{this.emisor.receptor.user.profile_image}}" alt="">
            <p>{{this.emisor.receptor.user.userName}}</p>
        </div>
        <div class="estado-user">

        </div>
    </div>
</a>
{{/each}}